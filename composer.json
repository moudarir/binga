{
  "name": "moudarir/binga",
  "description": "PHP library for integration with the Binga.ma API in a fast and simple way.",
  "keywords": [
    "php",
    "binga",
    "SDK",
    "library"
  ],
  "type": "library",
  "license": "MIT",
  "config": {
    "platform":{
      "php": "7.1.8"
    },
    "bin-dir": "vendor/bin"
  },
  "authors": [
    {
      "name": "Abdessamad MOUDARIR",
      "email": "amoudarir@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.8",
    "nesbot/carbon": "^2.6",
    "guzzlehttp/guzzle": "^6.3"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.0",
    "phpunit/phpunit": "^7.0",
    "ext-xdebug": "*"
  },
  "autoload": {
    "psr-4": {
      "Moudarir\\Binga\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Moudarir\\Binga\\Test\\": "src/tests"
    }
  },
  "minimum-stability": "dev"
}